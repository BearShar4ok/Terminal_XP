<Window x:Class="Terminal_XP.Windows.SputnikGameWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Terminal_XP.Windows"
        mc:Ignorable="d"
        Title="SputnikGameWindow" Height="450" Width="800">
    <Grid>
        <Canvas x:Name="mainField">
            <Rectangle x:Name="sputnik"></Rectangle>
            <Border x:Name="area"></Border>
            <TextBlock x:Name="timerZone"/>
        </Canvas>
    </Grid>
</Window>
